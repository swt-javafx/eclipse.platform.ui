<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

    <extension id="e4ide" point="org.eclipse.core.runtime.products"> 
      <product name="%productName" application="org.eclipse.e4.ui.workbench.swt.E4Application" description="%productBlurb"> 
          <property name="windowImages" value="eclipse16.gif,eclipse32.gif,eclipse48.gif,eclipse256.gif,eclipse16.png,eclipse32.png,eclipse48.png,eclipse256.png"/> 
          <property name="aboutImage" value="eclipse_lg.gif"/> 
          <property name="aboutText" value="%productBlurb"/> 
          <property name="appName" value="Eclipse"/> 
          <property name="preferenceCustomization" value="plugin_customization.ini"/>
          <property
          		name="introTitle"
          		value="%productIntroTitle"/>
          <property
          		name="introBrandingImage"
          		value="product:intro-eclipse.png"/>
          <property
          		name="introBrandingImageText"
          		value="%productIntroBrandingText"/>
		  <property
				name="introDescription-overview"
				value="%introDescription-overview"/>
		  <property
				name="introDescription-tutorials"
				value="%introDescription-tutorials"/>
		  <property
				name="introDescription-samples"
				value="%introDescription-samples"/>
		  <property
				name="applicationXMI"
				value="org.eclipse.platform/LegacyIDE.e4xmi">
		  </property>
		  <property
				name="cssTheme"
				value="org.eclipse.e4.ui.css.theme.e4_default">
		  </property>
		  <property
				name="applicationCSSResources"
          value="platform:/plugin/org.eclipse.ui.themes/images/">
		  </property>

          <property
        		name="startupForegroundColor"
        		value="9c9696"/>
          <property
                name="startupMessageRect"
                value="7,265,320,20"/>
          <property
                name="startupProgressRect"
                value="2,290,448,10"/>
          <property
                name="buildIdLocation"
                value="0,220">
          </property>
          <property
                name="buildIdSize"
                value="419,40">
          </property>
          <property
                name="lifeCycleURI"
                value="bundleclass://org.eclipse.ui.ide.application/org.eclipse.ui.internal.ide.application.IDELifeCycleManager">
          </property>
       </product>
    </extension> 

    <extension id="jfxide" point="org.eclipse.core.runtime.products"> 
      <product name="%productName" application="org.eclipse.fx.ui.workbench.fx.application" description="%productBlurb"> 
          <property name="windowImages" value="eclipse16.gif,eclipse32.gif,eclipse48.gif,eclipse256.gif,eclipse16.png,eclipse32.png,eclipse48.png,eclipse256.png"/> 
          <property name="aboutImage" value="eclipse_lg.gif"/> 
          <property name="aboutText" value="%productBlurb"/> 
          <property name="appName" value="Eclipse"/> 
          <property name="preferenceCustomization" value="plugin_customization.ini"/>
          <property
          		name="introTitle"
          		value="%productIntroTitle"/>
          <property
          		name="introBrandingImage"
          		value="product:intro-eclipse.png"/>
          <property
          		name="introBrandingImageText"
          		value="%productIntroBrandingText"/>
		  <property
				name="introDescription-overview"
				value="%introDescription-overview"/>
		  <property
				name="introDescription-tutorials"
				value="%introDescription-tutorials"/>
		  <property
				name="introDescription-samples"
				value="%introDescription-samples"/>
		  <property
				name="applicationXMI"
				value="org.eclipse.platform/LegacyIDE.e4xmi">
		  </property>
		  <property
				name="applicationCSSResources"
				value="platform:/plugin/org.eclipse.ui.themes/images/">
		  </property>
          <property
        		name="startupForegroundColor"
        		value="9c9696"/>
          <property
                name="startupMessageRect"
                value="7,265,320,20"/>
          <property
                name="startupProgressRect"
                value="2,290,448,10"/>
          <property
                name="buildIdLocation"
                value="0,220">
          </property>
          <property
                name="buildIdSize"
                value="419,40">
          </property>
          <property
                name="lifeCycleURI"
                value="bundleclass://org.eclipse.ui.ide.javafx/org.eclipse.ui.ide.javafx.IDELifeCycleManager">
          </property>
       </product>
    </extension> 

</plugin>
